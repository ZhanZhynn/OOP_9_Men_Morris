<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="680.0" prefWidth="500.0" style="-fx-background-color: #efebe5;" stylesheets="@application.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="game.Controller.SceneController">
   <children>
      <ImageView fitHeight="501.0" fitWidth="500.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../game_title.png" />
         </image>
      </ImageView>
      <Button layoutX="156.0" layoutY="512.0" mnemonicParsing="false" onAction="#switchToGameScene" style="-fx-min-width: 200; -fx-font-size: 20;" stylesheets="@button.css" text="Start">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button layoutX="156.0" layoutY="591.0" mnemonicParsing="false" onAction="#closeGame" style="-fx-min-width: 200; -fx-font-size: 20;" stylesheets="@button.css" text="Exit">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
